{
  "name": "@herb-tools/docs",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://docs.herb-tools.dev",
  "bugs": "https://github.com/marcoroth/herb/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcoroth/herb.git",
    "directory": "docs"
  },
  "scripts": {
    "dev": "vitepress dev",
    "build": "yarn vendor:playground && vitepress build",
    "vendor:playground": "cd ../playground && yarn build && cd - && cp ../playground/dist/index.html ./docs/public/playground-embed.html && cp -R ../playground/dist/assets ./docs/public",
    "preview": "vitepress preview",
    "clean": "rimraf .vitepress/dist/ && rimraf .vitepress/cache/ && rimraf ./docs/public/assets && rimraf ./docs/public/playground-embed.html"
  },
  "dependencies": {
    "@herb-tools/browser": "^0.0.0",
    "@herb-tools/core": "^0.0.0",
    "@herb-tools/node": "^0.0.0",
    "vitepress": "^2.0.0-alpha.4"
  },
  "devDependencies": {
    "@shikijs/vitepress-twoslash": "^3.1.0",
    "rimraf": "^6.0.1",
    "vitepress-plugin-group-icons": "^1.3.6"
  }
}
