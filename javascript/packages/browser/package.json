{
  "name": "@herb-tools/browser",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/herb-browser.esm.js",
  "module": "./dist/herb-browser.esm.js",
  "types": "./dist/types/src/index.d.ts",
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.json && node bin/build.js && emcc src/herb.c -o dist/libherb.js -s WASM=1 -s MODULARIZE=1 -s EXPORT_NAME=\"libherb\" -s EXPORTED_FUNCTIONS=\"['_herb_lex', '_herb_parse', '_herb_version']\" -s EXTRA_EXPORTED_RUNTIME_METHODS='[\"cwrap\"]'",
    "clean": "rimraf dist",
    "test": "vitest run"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/herb-browser.esm.js",
      "default": "./dist/herb-browser.esm.js"
    }
  },
  "dependencies": {
    "@herb-tools/core": "0.0.1"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "vitest": "^3.0.0",
    "typescript": "^5.8.2"
  }
}
