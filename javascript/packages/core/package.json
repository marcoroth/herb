{
  "name": "@herb-tools/core",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "main": "./dist/herb-core.cjs",
  "module": "./dist/herb-core.esm.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "yarn clean && rollup -c",
    "dev": "rollup -c -w",
